{
  "name": "contest_radar",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,css,html,json,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,md,css,html,json,yml,yaml}\"",
    "type-check": "tsc --noEmit",
    "commit": "cz",
    "commit:check": "commitlint --edit",
    "docker:dev": "./scripts/dev.sh",
    "docker:stop": "./scripts/stop.sh",
    "docker:build": "docker build -t contest-radar .",
    "docker:logs": "docker-compose logs -f",
    "docker:shell": "docker-compose exec app sh",
    "docker:db:shell": "docker-compose exec postgres psql -U contest_user -d contest_radar",
    "docker:cleanup": "docker-compose down -v --remove-orphans",
    "prepare": "husky"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.18.0",
