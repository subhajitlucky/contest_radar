{
  "name": "contest_radar",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,css,html,json,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,md,css,html,json,yml,yaml}\"",
    "type-check": "tsc --noEmit",
    "commit": "cz",
    "commit:check": "commitlint --edit",
    "docker:dev": "./scripts/dev.sh",
    "docker:stop": "./scripts/stop.sh",
    "docker:build": "docker build -t contest-radar .",
    "docker:logs": "docker-compose logs -f",
    "docker:shell": "docker-compose exec app sh",
    "docker:db:shell": "docker-compose exec postgres psql -U contest_user -d contest_radar",
    "docker:cleanup": "docker-compose down -v --remove-orphans",
    "prepare": "husky"
  },
  "dependencies": {
    "next": "16.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
